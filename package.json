{
  "name": "@jupyterlab/xkcd-extension",
  "version": "0.1.1",
  "description": "Show a random xkcd.com comic in a JupyterLab panel",
  "author": "Project Jupyter",
  "main": "lib/index.js",
  "homepage": "https://github.com/jupyterlab/jupyterlab_xkcd",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/jupyterlab/jupyterlab_xkcd.git"
  },
  "license" : "BSD-3-Clause",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run build",
    "watch": "tsc -w"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.12.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.0"
  }
}
