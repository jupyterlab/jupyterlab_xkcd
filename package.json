{
  "name": "@jupyterlab/xkcd-extension",
  "version": "0.1.0",
  "description": "Show a random xkcd.com comic in a JupyterLab panel",
  "keywords": [
    "extension",
    "jupyter",
    "jupyterlab"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab_xkcd",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab_xkcd/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab_xkcd.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.13.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.6.0"
  },
  "jupyterlab": {
    "extension": true
  }
}
